<?xml version="1.0" encoding="UTF-8"?>
<sequence name="SamplingRetry" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log level="custom">
        <property name="At start of SamplingRetrySeq" value="true"/>
    </log>
    <enrich>
        <source clone="true" type="envelope"/>
        <target property="SOAP_RECEIPT_COPY" type="property"/>
    </enrich>
    <log level="full"/>
    <property name="ContentType" expression="$trp:Content-Type"/>
    <log level="custom">
        <property name="Content-Type Header" expression="get-property('ContentType')"/>
    </log>
	<log level="custom">
        <property expression="$ctx:SOAP_RECEIPT_COPY" name="CARDEXCHANGE-MESSAGE-TO-PROCESS Received from ...EnqueueCardAPI_Sequence"/>
        <property name="At end of SamplingRetrySeq" value="true"/>
    </log>
</sequence>